@model System.Collections.Generic.List<WebMVC_Manha.Models.Musica>
@{
    ViewData["Title"] = "Bem-vindo (a)!";
}

<h2>@ViewData["Title"]</h2>

<h3>@ViewData["Mensagem"]</h3>

@foreach (var i in @model) {
<p><strong>Titulo</strong></p>
<p>@i.Titulo</p>
<p><strong>Estilo</strong></p>
<p>@i.Estilo</p>
<p><strong>Compositor</strong></p>
<p>@i.Compositor</p>
}